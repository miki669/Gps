<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="Gps.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:Gps.Views">
	<ShellContent
		Shell.NavBarIsVisible="False"
		Title="Main"
		ContentTemplate="{DataTemplate views:MainPage}"
		Route="MainPage" />

    <ShellContent 
        Shell.TabBarIsVisible="False"  
        Title="Track" 
        Route="track" 
        Shell.BackgroundColor="#2472BD"
        Shell.TabBarBackgroundColor="#2472BD"
        ContentTemplate="{DataTemplate views:TrackPage}" 

        />


</Shell>
